{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [ "es2018" ],
    "declaration": true,
    "declarationMap": false,
    "sourceMap": false,
    "outDir": "./lib",
    "removeComments": false,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": false,
    "baseUrl": "./",
    "paths": {
      "@wrserver/core": [ "../core/index.ts" ],
      "@wrserver/core/*": [ "../core/*" ],
      "@wrserver/crypt": [ "../crypt/index.ts" ],
      "@wrserver/crypt/*": [ "../crypt/*" ],
      "@wrserver/data": [ "../data/index.ts" ],
      "@wrserver/data/*": [ "../data/*" ],
      "@wrserver/mail": [ "../mail/index.ts" ],
      "@wrserver/mail/*": [ "../mail/*" ],
      "@wrserver/roles": [ "../roles/index.ts" ],
      "@wrserver/roles/*": [ "../roles/*" ]
    }
  },
  "files": [ "index.ts" ]
}